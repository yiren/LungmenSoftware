
@{
    ViewBag.Title = "相關工作站清單";
    
}

<h2>@ViewBag.softwareName安裝於以下工作站</h2>

<div>
    <h4>Fox Workstations</h4>
    <hr />
    <table class="table table-bordered table-hover table-striped">
        <tr>
            <th>@Html.DisplayName("工作站")</th>
        </tr>
        @foreach (var item in Model as System.Collections.IEnumerable)
        {
            <tr>
                <td>@Html.DisplayFor(model=>item.ToString())</td>

            </tr>
        }
            
    </table> 
</div>
<p>
    @Html.ActionLink("Back to List", "ListOfSoftwares")
</p>
